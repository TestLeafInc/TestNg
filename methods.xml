<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="5">
  <test thread-count="5" name="Book" parallel="methods">
    <classes>
      <class name="basics.Book">
      	<methods>
      		<include name=".*Book"></include>
      	</methods>      
      </class>
      <class name="basics.RunTests">
      	<methods>
      		<include name="walk"></include>
